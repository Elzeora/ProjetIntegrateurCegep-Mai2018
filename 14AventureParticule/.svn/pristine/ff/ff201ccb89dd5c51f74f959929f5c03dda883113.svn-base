package ressources;

import java.util.EventListener;

/**
 * 
 * @author Louis Roberge
 *
 */
public interface PanelSPListener extends EventListener{

	/**
	 * permet de changer la direction du systeme de plaques
	 * @param direction true vers le haut, false vers le bas
	 */
	public void changerDirectionSP(boolean direction);
	/**
	 * permet de changer la valeur du champ électrique du systeme de plaques
	 * @param valeur la valeur du champ électrique
	 */
	public void changerChampElectriqueSP(double valeur);
}
